{
  "name": "krypto.cash",
  "description": "Helps you figure out how your cryptocurrency investments are actually doing",
  "homepage": "https://www.krypto.cash",
  "version": "1.1.6",
  "main": "index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "not ie >= 10",
              "not ExplorerMobile >= 10"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "syntax-dynamic-import"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server",
    "render-shell": "babel-node ./render.js",
    "build": "webpack --progress --env.env=prod && yarn render-shell",
    "deploy": "yarn build && firebase deploy"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "incstr": "^1.2.3",
    "preact-render-to-string": "^3.7.0",
    "serviceworker-webpack-plugin": "^0.2.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@firebase/app": "^0.1.2",
    "@firebase/database": "^0.1.3",
    "classnames": "^2.2.5",
    "ganalytics": "^2.0.1",
    "idb": "^2.0.4",
    "preact": "^8.2.6",
    "preact-async-route": "^2.0.1",
    "preact-router": "^2.6.0"
  }
}
